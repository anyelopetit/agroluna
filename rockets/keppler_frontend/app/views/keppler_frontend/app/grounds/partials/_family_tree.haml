.col-12.col_family-tree
  .col-lg-2.content_family_tree
    .card_status_specific.block_one.family_tree
      .parent_card
        - if @parent = @ground
          = link_to farm_ground_path(@farm, @ground) do
            .parent_img
              = image_tag ('https://image.flaticon.com/icons/svg/523/523448.svg'),style: "width: 48px;", class: 'avatar_ground'
            %dl
              %dt{:style => "font-weight: 700; color: #6f6f6f;"}
                = @ground.identifier
              %dt{:style => "font-weight:400; color: #6f6f6f;"} campo
        - else
          %h3 HIJO
