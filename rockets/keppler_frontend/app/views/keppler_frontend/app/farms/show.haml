-# = render 'keppler_frontend/app/farms/components/navbar', farm: @farm, farms: @farms
// = render 'keppler_frontend/app/farms/components/profile-banner', farm: @farm
.app_wrapper
  %img.bg-shape2{src:'/assets/keppler_frontend/app/shape2.svg'}
  .container_custom
    .row
      .col-lg-12
        .description_land
          %h1 Descripci√≥n de la finca
          %p= raw @farm&.description
      .col-lg-12
        .photos_land
          .items
            - @farm&.photos.each do |photo|
              .carousel_img
                = image_tag (photo&.photo&.url || 'https://images.pexels.com/photos/787647/pexels-photo-787647.jpeg?auto=compress&cs=tinysrgb&dpr=2&h=650&w=940')
          .multiple-items
            - @farm&.photos.each do |photo|
              .carousel_img_multiple
                = image_tag (photo&.photo&.url || 'https://images.pexels.com/photos/787647/pexels-photo-787647.jpeg?auto=compress&cs=tinysrgb&dpr=2&h=650&w=940')